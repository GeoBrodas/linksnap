generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["referentialIntegrity"]
}

datasource db {
    provider             = "mysql"
    url                  = env("PLANETSCALE_PRISMA_DATABASE_URL")
    referentialIntegrity = "prisma"
}

model user {
    id          Int       @id @default(autoincrement())
    name        String
    occupation  String
    email       String
    country     String
    github      String    @unique
    twitter     String?
    linkedin    String?
    facebook    String?
    hashnode    String?
    devto       String?
}